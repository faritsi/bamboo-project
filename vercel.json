{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.5.5" }
    },
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },

        {
            "src": "/php/",
            "dest": "/public/php/$1"
        },
        {
            "src": "/favicon/(.*)",
            "dest": "/public/favicon/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "",
